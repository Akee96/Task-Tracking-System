@{
    string currentUserServer = (Request.ServerVariables["LOGON_USER"]) != "" ? Request.ServerVariables["LOGON_USER"] : System.Security.Principal.WindowsIdentity.GetCurrent().Name;

    string currentUser = currentUserServer;
    string currentUserId = currentUser.Substring(currentUserServer.LastIndexOf("\\", StringComparison.Ordinal) + 1).ToUpper();

    if (currentUserId == "AKALANKA")
    {
        currentUserId = "SP7403";
    }
}


